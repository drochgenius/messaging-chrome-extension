{
  "name": "messaging-chrome-extension",
  "version": "1.0.0",
  "description": "A Chrome extension that establishes a bidirectional channel between 2 web applications",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf coverage dist tmp",
    "build": "tsc",
    "watch": "tsc -w -p tsconfig.release.json",
    "lint": "tslint -t stylish --project \"tsconfig.json\"",
    "pretest": "npm run lint",
    "test": "npm run test-only",
    "test-only": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/drochgenius/messaging-chrome-extension.git"
  },
  "keywords": [],
  "author": "Dominique Rochefort <dominique.rochefort@hmhco.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/drochgenius/messaging-chrome-extension/issues"
  },
  "homepage": "https://github.com/drochgenius/messaging-chrome-extension#readme",
  "engineStrict": true,
  "engines": {
    "node": ">= 8.9.0"
  },
  "dependencies": {
    "@types/chrome": "0.0.65",
    "tslib": "~1.9.0"
  },
  "devDependencies": {
    "@types/jest": "~22.2.3",
    "@types/node": "~8.10.10",
    "jest": "~22.4.3",
    "rimraf": "~2.6.2",
    "ts-jest": "~22.4.4",
    "tslint": "~5.9.1",
    "tslint-microsoft-contrib": "~5.0.3",
    "tsutils": "~2.26.2",
    "typescript": "~2.8.3"
  }
